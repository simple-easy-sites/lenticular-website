
{
  "name": "lenticular-ai-landing",
  "version": "1.0.0",
  "description": "Landing page for Lenticular AI",
  "private": true,
  "scripts": {
    "dev": "echo \"No dev script configured, serve index.html with a local server. Consider using a simple esbuild dev server.\" && exit 0",
    "build": "esbuild index.tsx --bundle --outfile=index.js --platform=browser --format=esm --jsx=automatic --external:react --external:react-dom --external:react-dom/ --external:react/ --sourcemap --log-level=info",
    "start": "echo \"No start script configured.\" && exit 0"
  },
  "keywords": [
    "lenticular",
    "ai",
    "landing-page",
    "vercel"
  ],
  "author": "Lenticular AI Team",
  "license": "UNLICENSED",
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.0.0",
    "esbuild": "^0.20.0"
  }
}